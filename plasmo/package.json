{
  "name": "plasmo",
  "displayName": "Plasmo",
  "version": "0.0.1",
  "description": "complete text everywhere",
  "author": "jared graham",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@clerk/chrome-extension": "^2.4.0",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "^7.5.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "key": "$CRX_PUBLIC_KEY",
    "host_permissions": [
      "$PLASMO_PUBLIC_CLERK_SYNC_HOST/*",
      "$CLERK_FRONTEND_API/*"
    ],
    "permissions": [
      "tabs",
      "storage",
      "activeTab",
      "cookies"
    ],
    "background": {
      "service_worker": "background/index.ts",
      "type": "module"
    }
  }
}
